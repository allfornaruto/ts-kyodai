{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["ESNext", "DOM"],
    "target": "es5",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "outDir": "temp/vscode-dist",
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["./node_modules/@types", "./assets/scripts/types"]
  },
  "exclude": ["node_modules", "library", "local", "temp", "build", "settings", "packages"]
}
